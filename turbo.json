{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_RECAPTCHA_KEY",
        "NEXT_PUBLIC_ENABLE_BETA",
        "NEXT_PUBLIC_API_URL"
      ],
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "vercel": {
      "cache": false
    }
  }
}
